{
  "presets": ["es2015", "es2016", "es2017", "react"],
  "plugins": [
    ["module-resolver", { "root": ["./"] }],
    ["transform-decorators-legacy"],
    ["transform-class-properties"],
    ["transform-object-rest-spread"],
    ["add-module-exports"],
    ["transform-runtime", { "polyfill": false }]
  ]
}
